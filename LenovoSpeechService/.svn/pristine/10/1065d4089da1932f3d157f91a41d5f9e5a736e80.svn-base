package cn.com.lenovo.speechservice.domain;

import java.util.ArrayList;

/**
 * 保存返回的电影、视频、菜谱类型的数据
 * @author kongqw
 *
 */
public class EtityInfo {
	// 状态码
	public int code;
	// 文字内容
	public String text;
	// 实体信息
	public ArrayList<Etity> list;

	/**
	 * 航班信息
	 * @author kongqw
	 *
	 */
	public class Etity {
		// 名称
		public String name;
		// 详情
		public String info;
		// 详情地址
		public String detailurl;
		// 图标地址
		public String icon;
	}
}
