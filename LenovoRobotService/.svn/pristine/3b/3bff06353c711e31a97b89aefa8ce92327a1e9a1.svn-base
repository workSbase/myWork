package com.lenovo.main.TaskInterface;

/**
 * 其他任务接口
 * 
 * @author Administrator
 * 
 */
public interface AnotherInterface {
	/**
	 * 剩余电量
	 * 
	 * @return
	 */
	int getRobotBatteryCap();// 返回值为当前剩余电量百分比乘以100

	/**
	 * 触摸机器人的值
	 * 
	 * @return
	 */
	int getTouchSensorStauts();// 返回值为1,2,3,4分别对应没有触摸,单触，多次触，长触

	/**
	 * 打开LED
	 * 
	 * @return
	 */
	int serLEDOn();

	/**
	 * 关闭LED
	 * 
	 * @return
	 */
	int setLEDOff();

	/**
	 * 开始获取触摸值
	 * 
	 * @return
	 */
	int startGetTouchResult();

	/**
	 * 关闭获取触摸值
	 */
	void closeGetTouchResult();
}
